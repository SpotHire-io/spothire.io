  "scripts": {
    "start": "node scripts/start.js",
    "build": "tsc && node scripts/build.js",
    "lint": "eslint src",
    "test": "node scripts/test.js --env=jsdom",
    "upload": "aws s3 cp --recursive storybook-static/ s3://spothire.io --acl public-read",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "tsc && build-storybook -s public",
    "watch-css": "postcss -w --config postcss.config.js --output public/css/app.css assets/css/app.css",
    "build-css": "postcss --config postcss.config.js --output public/css/app.css assets/css/app.css",
    "watch-ts": "tsc -w"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
